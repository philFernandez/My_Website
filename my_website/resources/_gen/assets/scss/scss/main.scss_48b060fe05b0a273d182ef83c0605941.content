*{box-sizing:border-box}html{font-family:oswald,sans-serif}body{background-color:#e0e0e0;color:#000;font-size:10px}h1.header{font-size:3.4rem;margin:18px 0 0}h2{font-size:1.4rem}a{color:#000;text-decoration:none}div.title p{margin:0;font-family:source code pro,monospace;font-size:1.4rem}div.top-container{text-align:center}i.fa-envelope,i.fa-phone{position:relative;top:3px;right:7px}.container{font-family:source code pro,monospace;font-size:1rem}@media only screen and (min-width:768px){div.item1>p,div.item2>p{font-size:1.1rem;padding:14px;background-color:#eee;box-shadow:5px 5px 5px lightgrey;border-radius:5px}div.item3>ul,div.item4>ul{font-size:1.1rem;padding:14px 14px 14px 28px;background-color:#eee;box-shadow:5px 5px 5px lightgrey;border-radius:5px}.grid1-container{display:grid;border:1px solid grey;font-family:source code pro,monospace;grid-template-columns:152px 152px 152px 152px 152px 152px 152px 152px 152px 152px 152px 152px;column-gap:8px}.item1,.item2{grid-column-start:2;grid-column-end:4}.item3{grid-row-start:1;grid-column-start:6;grid-column-end:8}.item4{grid-row-start:2;grid-column-start:6;grid-column-end:8}}